  					  <%= text_field "member", "username" %>
  					  <label for="member_password">Password</label> <strong><a href="#">?</a></strong>
  					  <%= password_field "member", "password" %>
  					  <%= submit_tag "Login" %>&nbsp;
  					  <label for="remember_me">Remember Me</label>
  					  <%= check_box_tag "remember_me", :checked => 'checked' %>&nbsp;
  					<%- end -%>
